{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api/|_next/|static/|assets/|favicon.ico|sw.js|.*\\..*).*)",
      "destination": "/index.html"
    }
  ]
}